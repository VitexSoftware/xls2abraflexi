{
    "name": "vitexsoftware/xls2abralexi",
    "description": "Custom XLS Invoice Importer for AbraFlexi",
    "type": "project",
    "require": {
        "spojenet/flexibee": "dev-main",
        "phpoffice/phpspreadsheet": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "11.4.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Utopia\\Xls2abralexi\\": "src/Utopia/Xls2abraflexi/"
        }
    },
    "authors": [
        {
            "name": "CyberVitexus",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "*",
        "friendsofphp/php-cs-fixer": "^3.61",
        "ergebnis/composer-normalize": "^2.43",
        "ergebnis/php-cs-fixer-config": "^6.34"
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
